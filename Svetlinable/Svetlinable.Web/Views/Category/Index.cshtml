@model Svetlinable.Web.ViewModels.CategoryViewModels.CategoryWrapperViewModel

<h1>Index Page:</h1>
@{ int a = 5;
}
<h2>@{a++;}</h2>

<table class="table table-hover" id="categoryListingTable">
    <tbody>
        @foreach (var category in Model.CategoryListing)
        {
            <tr>
                <td>
                    @category.Title
                </td>
                <td>
                    @category.Description
                </td>
            </tr>
        }
    </tbody>
</table>